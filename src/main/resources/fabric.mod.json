  "authors": [
    "Teh Flamin' Taco"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/pathcraft/icon.png",

  "environment": "*",
